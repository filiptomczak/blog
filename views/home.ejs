<%-include("partials/header");-%>
<h1>Home</h1>

<p><%=startingContent%></p>

<%postsHome.forEach(function(element){%>
  <h1><%=element.title;%></h1>
  <p>
    <%=element.content.substring(0,99)+"...";%> 
    <a class="post-href" href="/post/<%=element.title%>">Read more</a>
  </p>
<%})%>

<%-include("partials/footer");-%>
